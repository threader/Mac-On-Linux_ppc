LN_S		= @LN_S@
CFLAGS		= @CFLAGS@
WFLAGS		= -Wall -Wmissing-prototypes
CC		= @CC@
LD		= @LD@
AS		= @AS@
M4		= @M4@
NM		= @NM@
CPP		= @CPP@

DEPEXTRA	= @DEPEXTRA@
